<html>
    <body>
        <h1>Noticias</h1>
    
        <table>
            <tr>
                <td>ID</td>
                <td>Titulo</td>
                <td>Noticia</td>
                <td>Hora</td>
            </tr>

            <% for(var i=0; i <noticias.length; i++) {%>
            <tr>
                <td><%= noticias[i][0] %></td>
                <td><%= noticias[i][1] %></td>
                <td><%= noticias[i][2] %></td>
                <td><%= noticias[i][3] %></td>
            </tr>
            <% } %>
        </table>
    </body>
</html>